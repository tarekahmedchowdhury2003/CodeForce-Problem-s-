#include <iostream>
#include <cstring>
using namespace std;

int main() {
    
    char str1[110],str2[110];
    
    cin>>str1;
    cin>>str2;
    
    for (int i=0; i<strlen(str1); i++){
        char ch1 = tolower(str1[i]);
        char ch2 = tolower(str2[i]);
        
        
        if(ch1 > ch2){
            cout<<"1"<<endl;
            return 0;
        }else if(ch1 < ch2){
            cout<<"-1"<<endl;
            return 0;
        }
    }
    cout<<"0"<<endl;
    

    return 0;
}
